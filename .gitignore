# Ignore directories generated by Composer
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/

# Ignore Drupal's temporary files
/tmp/
/web/sites/*/files/
/web/sites/default/files/
/web/default/private/

# Ignore settings.php and services.yml
/web/sites/default/settings.php
/web/sites/default/services.yml

# Ignore development environment files
/web/sites/*/settings.local.php
/web/sites/*/development.services.yml

# Ignore database backups
*.sql
*.sqlite
*.sql.gz

# Ignore compiled styles
*.css.map
*.scssc

# Ignore logs and debug output
*.log
*.out

# Ignore IDE-specific files
.idea/
.vscode/

# Ignore node_modules (if you use npm/yarn for frontend)
node_modules/
npm-debug.log

# Ignore MacOS and system-specific files
.DS_Store
Thumbs.db
